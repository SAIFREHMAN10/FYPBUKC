<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Bootstrap -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous" />
    <script src="https://kit.fontawesome.com/7f4c749960.js" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="../../css/style.css" />
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <title>Document</title>
</head>

<body>

    <div class="wrapper-navabr">
        <nav>
            <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
                <div class="container-fluid">
                    <a class="navbar-brand" href="#">FYP LMS 2022</a>
                    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse" id="navbarSupportedContent">
                        <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                            <li class="nav-item">
                                <a class="nav-link" aria-current="page" href="../instructor/instrucotrCourseView.html">Courses</a>
                            </li>

                        </ul>
                        <form class="d-flex">
                            <li class="nav-item dropdown">
                                <a type="button" class="btn btn-danger badge " href="/">
                                    <span class="h5">Sign Out</span>
                                </a>

                            </li>
                        </form>
                    </div>
                </div>
            </nav>
        </nav>
    </div>


    <!-- Navbar Ended -->


    <!-- Main Body -->
    <div class="container-fluid">
        <div class="row">
            <div class="col">
                <br>
                <hr>
                <h2>Submitted Quiz</h2>
                <hr>
            </div>

        </div>
    </div>


    <!-- Current Courses -->
    <div class="container mt-5">
        <table class="table-responsive table table-hover">
            <thead>
                <tr>
                    <th scope="col">Quiz #</th>
                    <th scope="col">Quiz Tittle</th>
                    <th scope="col">Submitted File</th>
                    <th scope="col">Download</th>


                </tr>
            </thead>
            <tbody>
                <%movieList.forEach(movie=> {%>
                <tr>
                    <th scope="row"><%= movie.QNO %></th>
                    <td><%= movie.QNA %></td>
                    <td><%= movie.img %></td>
<td>
    <form action="/download4/<%=movie._id %>">
        <input type="submit" value="Download" class="btn btn-dark">
       </form>
</td>
                </tr>
               
                <%})%>
            </tbody>
            
        </table>
    </div>



    <!-- Bootsrap -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    <script src="../index.js"></script>
</body>

</html>