    <!-- Main Body -->
    <div class="container">
        <div class="row mt-4">
            <div class="col">
                <hr>
                <h2>Quizes</h2>
                <hr>

            </div>
        </div>
    </div>

    <!-- ///////////////////////////////////                Table-Lecture                     ///////////////////////////////////// -->

    <div class="container">
        <div class="accordion" id="accordionExample">
            <div class="accordion-item">
                <h2 class="accordion-header" id="headingOne">
                    <button class="accordion-button" type="button" data-bs-toggle="collapse"
                        data-bs-target="#studentCourseCollapse1" aria-expanded="true" aria-controls="collapseOne">
                        Quiz
                    </button>
                </h2>
                <div id="studentCourseCollapse1" class="accordion-collapse collapse show" aria-labelledby="headingOne"
                    data-bs-parent="#accordionExample">
                    <div class="accordion-body">
                        <div class="row table-responsive">
                            <table class="table table-hover">
                                <thead>
                                    <tr>
                                        <th scope="col">Week#</th>

                                        <th scope="col">Quiz Title</th>
                                        <th scope="col">Download Quiz FIle</th>
                                        <th scope="col">Upload Quiz File</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <%movieList.forEach(movie=> {%>
                                        <tr>
                                            <th scope="row">
                                                <%= movie.quizno %>
                                            </th>

                                            <td>
                                                <%= movie.Qname %>
                                            </td>

                                            <td><a href="../../../../../LMSFYP/public/upload/image/images.jpg"
                                                    class="btn btn-dark">Download</a> </td>
                                            <td>
                                                

                                                <div class="">
                                                    <a href="" class="btn btn-dark " data-bs-toggle="modal"
                                                        data-bs-target="#studentdUploadQuiz">
                                                        Submit
                                                        Quiz</a>

                                                    <!-- Modal -->
                                                    <div class="modal fade" id="studentdUploadQuiz" tabindex="-1"
                                                        aria-labelledby="exampleModalLabel" aria-hidden="true">
                                                        <div class="modal-dialog">
                                                            <div class="modal-content">
                                                                <div class="modal-header">
                                                                    <h5 class="modal-title" id="exampleModalLabel">
                                                                        Submit Quiz</h5>
                                                                    <button type="button" class="btn-close"
                                                                        data-bs-dismiss="modal"
                                                                        aria-label="Close"></button>
                                                                </div>
                                                                <div class="modal-body">
                                                                    <form action="/studentQuizView.ejs" method="POST"
                                                                        enctype="multipart/form-data">

                                                                        <h4 class="h4">Upload your Quiz</h4>
                                                                        <hr>
                                                                        <div class="input-group col-md-6">
                                                                            <div
                                                                                class="input-group input-group-sm mb-3">
                                                                                <div class="input-group-prepend">
                                                                                    <span class="input-group-text"
                                                                                        id="inputGroup-sizing-sm">Quiz#</span>
                                                                                </div>
                                                                                <input type="text" class="form-control"
                                                                                    aria-label="Small" name="QNO"
                                                                                    aria-describedby="inputGroup-sizing-sm"
                                                                                    value="<%= movie.quizno %>">
                                                                            </div>
                                                                            <!-- <p class="p">Quiz #<%= movie.quizno %></p> -->

                                                                        </div>
                                                                        <div class="input-group col-md-6">
                                                                            <div
                                                                                class="input-group input-group-sm mb-3">
                                                                                <div class="input-group-prepend">
                                                                                    <span class="input-group-text"
                                                                                        id="inputGroup-sizing-sm">Quiz
                                                                                        Tittle</span>
                                                                                </div>
                                                                                <input type="text" class="form-control"
                                                                                    aria-label="Small" name="QNA"
                                                                                    aria-describedby="inputGroup-sizing-sm"
                                                                                    value="<%= movie.Qname %>">
                                                                            </div>
                                                                            <!-- <p class="p">Quiz Tittle<%= movie.Qname %></p> -->

                                                                        </div>
                                                                        <hr>
                                                                        <div class="input-group col-md-6">
                                                                            <div class="custom-file">
                                                                                <label class="custom-file-label"
                                                                                    for="inputGroupFile04"></label>
                                                                                <br>
                                                                                <input type="file"
                                                                                    class="custom-file-input"
                                                                                    name="image" id="inputGroupFile04">

                                                                            </div>

                                                                        </div>



                                                                </div>
                                                                <div class="modal-footer">
                                                                    <button type="button" class="btn btn-light"
                                                                        data-bs-dismiss="modal">Close</button>
                                                                    <button type="submit"
                                                                        class="btn btn-dark">Upload</button>
                                                                    </form>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>


                                            </td>
                                        </tr>

                                        <%})%>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>



        </div>




        <!-- Bootsrap -->
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
            crossorigin="anonymous"></script>
        <script src="../index.js"></script>
</body>

</html>